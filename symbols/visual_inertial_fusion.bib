% Encoding: UTF-8

@symbol{bodyFrame,
    name = {\ensuremath{\mathcal{B}}},
    description = {body reference frame},
    identifier = {visualInertialFusion}
}

@symbol{angVelMeas_InBodyFrame,
    name = {\ensuremath{\tilde{\boldsymbol{\omega}}}},
    description = {angular velocity measurement (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{linVelMeas_InBodyFrame,
    name = {\ensuremath{\tilde{\mathbf{a}}}},
    description = {linear acceleration measurement (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{imuMeasInterval_InBodyFrame,
    name = {\ensuremath{\mathbf{u}=\{\tilde{\boldsymbol{\omega}},\tilde{\mathbf{a}}\}}},
    description = {IMU measurements in time interval $[t_{k-1},\gls{timeK}]$ (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{angVel_InBodyFrame,
    name = {\ensuremath{\boldsymbol{\omega}}},
    description = {angular velocity (true value) (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{linVel_InBodyFrame,
    name = {\ensuremath{\mathbf{a}}},
    description = {linear acceleration (true value) (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{biasGyro_InBodyFrame,
    name = {\ensuremath{\mathbf{b}^G}},
    description = {gyroscope bias (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{biasAccel_InBodyFrame,
    name = {\ensuremath{\mathbf{b}^A}},
    description = {accelerometer bias (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{noiseGyro_InBodyFrame,
    name = {\ensuremath{\mathbf{n}^G}},
    description = {gyroscope additive zero-mean Gaussian white noise (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{noiseAccel_InBodyFrame,
    name = {\ensuremath{\mathbf{n}^A}},
    description = {accelerometer additive zero-mean Gaussian white noise (expressed in \gls{bodyFrame})},
    identifier = {visualInertialFusion}
}

@symbol{PsvRotWtoB,
    name = {\ensuremath{\mathbf{R}_\mathcal{BW}}},
    description = {passive rotation from \gls{worldFrame} to \gls{bodyFrame}},
    identifier = {visualInertialFusion}
}

@symbol{accelGrav_InWorldFrame,
    name = {\ensuremath{\mathbf{g}}},
    description = {gravity vector (expressed in \gls{worldFrame})},
    identifier = {visualInertialFusion}
}

@symbol{stdBiasDeriv,
    name = {\ensuremath{\sigma_b}},
    description = {standard deviation of zero-mean Gaussian noise bias derivative},
    identifier = {visualInertialFusion}
}

@symbol{pos_InWorldFrame,
    name = {\ensuremath{\mathbf{p}}},
    description = {Position (expressed in \gls{worldFrame})},
    identifier = {visualInertialFusion}
}

@symbol{att_InWorldFrame,
    name = {\ensuremath{\mathbf{q}}},
    description = {orientation/attitude of IMU (rotation $\mathbf{R}_\mathcal{WB}$)},
    identifier = {visualInertialFusion}
}

@symbol{vel_InWorldFrame,
    name = {\ensuremath{\mathbf{v}}},
    description = {velocity (expressed in \gls{worldFrame})},
    identifier = {visualInertialFusion}
}

@symbol{stateEst_InWorldFrame,
    name = {\ensuremath{\mathbf{x}=[\mathbf{p}\ \mathbf{q}\ \mathbf{v}]^\top}},
    description = {state estimate (expressed in \gls{worldFrame})},
    identifier = {visualInertialFusion}
}

@symbol{noCamPoses,
    name = {\ensuremath{N}},
    description = {number of camera poses/state estimates},
    identifier = {visualInertialFusion}
}

@symbol{noLandmarks,
    name = {\ensuremath{M}},
    description = {number of landmarks/features},
    identifier = {visualInertialFusion}
}

@symbol{stateEstSet_InWorldFrame,
    name = {\ensuremath{\mathbf{X}=\{\mathbf{x}_k\}}},
    description = {Set of state estimates at times \gls{timeK}, $k=1,\dots,N$},
    identifier = {visualInertialFusion}
}

@symbol{stateCov,
    name = {\ensuremath{\mathbf{\Lambda}_k}},
    description = {covariance of $\mathbf{x}_k$ at time \gls{timeK}},
    identifier = {visualInertialFusion}
}

@symbol{timeK,
    name = {\ensuremath{t_k}},
    description = {time at iteration $k$},
    identifier = {visualInertialFusion}
}

@symbol{idxk,
    name = {\ensuremath{(\cdot)_k}},
    description = {index for \gls{timeK}},
    identifier = {visualInertialFusion}
}

@symbol{landmarksSet,
    name = {\ensuremath{\mathbf{L}=\{\mathbf{L}^i\}}},
    description = {Set of 3D landmarks (expressed in \gls{worldFrame}), $i=1,\dots,M$},
    identifier = {visualInertialFusion}
}

@symbol{features2D,
    name = {\ensuremath{\mathbf{z}^i}},
    description = {observed (2D) features, $i=1,\dots,M$},
    identifier = {visualInertialFusion}
}

@symbol{featureCov,
    name = {\ensuremath{\mathbf{\Sigma}_k^i}},
    description = {covariance of \gls{feature2D} at time \gls{timeK}},
    identifier = {visualInertialFusion}
}

@symbol{TODO,
    name = {\ensuremath{\pi(\mathbf{x}_k,\mathbf{L}^i)}},
    description = {perspective projection of $\mathbf{L}^i$ onto image plane (expressed in \gls{cameraFrame} defined by $\mathbf{x}_k$)},
    identifier = {visualInertialFusion}
}