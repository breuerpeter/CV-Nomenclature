% Encoding: UTF-8

@symbol{worldFrame,
    name = {\ensuremath{\mathcal{\color{blue}W}}},
    description = {fixed, stationary world reference frame},
    identifier = {frameTransformation}
}

@symbol{cameraFrame,
    name = {\ensuremath{\mathcal{\color{red}C}}},
    description = {potentially moving camera reference frame},
    identifier = {frameTransformation}
}

@symbol{frameA,
    name = {\ensuremath{\mathcal{A}}},
    description = {arbitrary reference frame},
    identifier = {frameTransformation}
}

@symbol{frameB,
    name = {\ensuremath{\mathcal{B}}},
    description = {arbitrary reference frame},
    identifier = {frameTransformation}
}

@symbol{point3D,
    name = {\ensuremath{P}},
    description = {arbitrary point in 3D},
    identifier = {frameTransformation}
}

@symbol{point3D_InFrameA,
    name = {\ensuremath{_\mathcal{A}\mathbf{P}}},
    description = {coordinate vector of \gls{point3D} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}

@symbol{point3D_InFrameA_X,
    name = {\ensuremath{X^\mathcal{A}}},
    description = {x coordinate of \gls{point3D_InFrameA}},
    identifier = {frameTransformation}
}

@symbol{point3D_frameA_Y,
    name = {\ensuremath{Y^\mathcal{A}}},
    description = {y coordinate of \gls{point3D_InFrameA}},
    identifier = {frameTransformation}
}

@symbol{point3D_InFrameA_Z,
    name = {\ensuremath{Z^\mathcal{A}}},
    description = {z coordinate of \gls{point3D_InFrameA}},
    identifier = {frameTransformation}
}

@symbol{point3D_InFrameA_Hom,
    name = {\ensuremath{_\mathcal{A}\tilde{\mathbf{P}}}},
    description = {homogeneous coordinate vector of \gls{point3D} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}

@symbol{PsvRotAtoB,
    name = {\ensuremath{\mathbf{R}_\mathcal{AB}}},
    description = {passive rotation matrix from \gls{frameB} to \gls{frameA}},
    identifier = {frameTransformation}
}

@symbol{PsvRotBto1,
    name = {\ensuremath{\mathbf{R}_{1\mathcal{B}}}},
    description = {passive elementary rotation (about a single axis) from \gls{frameB} to an intermediate reference frame $1$},
    identifier = {frameTransformation}
}

@symbol{PsvRot1to2,
    name = {\ensuremath{\mathbf{R}_{21}}},
    description = {passive elementary rotation (about a single axis) from an intermediate reference frame $1$ to $2$},
    identifier = {frameTransformation}
}

@symbol{PsvRot2toA,
    name = {\ensuremath{\mathbf{R}_{\mathcal{A}2}}},
    description = {passive elementary rotation (about a single axis) from an intermediate reference frame $2$ to \gls{frameA}},
    identifier = {frameTransformation}
}

@symbol{TranslAtoB_InFrameA,
    name = {\ensuremath{_\mathcal{A}\mathbf{t}_\mathcal{AB}}},
    description = {translation from origin of \gls{frameA} to origin of \gls{frameB} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}

@symbol{HomTransBtoA,
    name = {\ensuremath{\mathbf{T}_\mathcal{AB}}},
    description = {homogeneous transformation from \gls{frameB} to \gls{frameA}},
    identifier = {frameTransformation}
}

@symbol{frameB_BasVecX_InFrameA,
    name = {\ensuremath{_\mathcal{A}\mathbf{e}_x^\mathcal{B}}},
    description = {x basis vector of \gls{frameB} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}

@symbol{frameB_BasVecY_InFrameA,
    name = {\ensuremath{_\mathcal{A}\mathbf{e}_y^\mathcal{B}}},
    description = {y basis vector of \gls{frameB} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}

@symbol{frameB_BasVecZ_InFrameA,
    name = {\ensuremath{_\mathcal{A}\mathbf{e}_z^\mathcal{B}}},
    description = {z basis vector of \gls{frameB} (expressed in \gls{frameA})},
    identifier = {frameTransformation}
}